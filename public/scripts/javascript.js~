function leftArrowPressed() {
  document.getElementById('B').click();
}

function rightArrowPressed() {
  document.getElementById('B').click();
}

document.onkeydown = function(evt) {
    evt = evt || window.event;
    switch (evt.keyCode) {
        case 37:
            leftArrowPressed();
            break;
        case 39:
            rightArrowPressed();
            break;
    }

};


